# BASE10 Vault - Environment Configuration
# Copy this file to .env and fill in your values
# DO NOT COMMIT .env TO GIT!

# ==================================
# RPC URLs
# ==================================

# Base Sepolia Testnet
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org

# Base Mainnet (for future use)
BASE_MAINNET_RPC_URL=https://mainnet.base.org

# ==================================
# Private Keys
# ==================================

# Your deployer private key (without 0x prefix)
# CRITICAL: Never commit this file with real keys!
PRIVATE_KEY=your_private_key_here_without_0x

# ==================================
# API Keys for Contract Verification
# ==================================

# Basescan API Key
# Get from: https://basescan.org/myapikey
BASESCAN_API_KEY=your_basescan_api_key_here

# Etherscan API Key (Base uses Etherscan infrastructure)
# Get from: https://etherscan.io/myapikey
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# ==================================
# Deployed Contract Addresses
# ==================================

# After deployment, fill these in for easy reference
VAULT_ADDRESS=
IMPLEMENTATION_ADDRESS=
ORACLE_ADDRESS=
SWAP_ROUTER_ADDRESS=

# ==================================
# Configuration
# ==================================

# Gas price settings (optional)
# GAS_PRICE=1000000000

# Confirmation blocks (optional)
# CONFIRMATIONS=2
